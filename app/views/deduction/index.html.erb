<%= content_for :header do %>
<%= render :partial => "/employees/emp_hub"%>
<% end %>
<div id="empty">
</div>
<div id="fieldset">
<fieldset id="fieldsetcon">
<legend>Search</legend>
<%= form_tag deduction_index_path, :method => 'get' do %>
<table>
<tr><td><%= text_field_tag :search, params[:search], :class =>"search_input" %></td>
<td><input type="submit" value="Search", id="button"></td></tr>
</table>
<% end %>
</fieldset>
</div>
<table id="search_employees">
    <tbody>
         <tr>
             <th>Deduction ID</th>
             <th>Deduction Type</th>
		<th>Deduction Mode</th>
	    <th>Section</th>
             <th>Financial Year</th>
<th>Edit</th>
          </tr>
<% @deduction.each do |deduct| %>

	<tr id="emp_<%=deduct.id%>">
      	<td><%= link_to deduct.id, :action => "show", :id =>deduct.id %></td>
           <td><%=deduct.deduction_type%></td>
<td><%=deduct.deduction_mode%></td>
<td><%=deduct.section%></td>
<td><%=deduct.financial_year%></td>
      	<td><%= link_to 'edit', :action => "edit", :id =>deduct.id %></td>
          </tr>
     <% end %>
   </tbody>
</table>

<%= will_paginate @deduction%>




